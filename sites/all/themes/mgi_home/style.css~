background: -webkit-linear-gradient(left,  rgba(250,251,251,0.4) 46%,rgba(250,250,250,0.4) 90%,rgba(241,242,242,1) 99%,rgba(238,239,239,1) 100%); /* Chrome10+,Safari5.1+ */
